{
  "name": "pandastix/wordpress2drupal",
  "description": "Wordpress to Drupal content migration",
  "type": "project",
  "require": {
    "querypath/querypath": "^3.0",
    "symfony/console": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "mockery/mockery": "^0.9.5"
  },
  "bin": [
    "bin/console"
  ],
  "autoload": {
    "files": [
      "src/functions_include.php"
    ],
    "psr-4": {
      "Wordpress2Drupal\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Joseph Zhao",
      "email": "joseph.zhao@xing.net.au"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "secure-http": false
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
